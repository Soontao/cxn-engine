{
  "name": "cxn-engine",
  "version": "0.0.3",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "description": "execute CXN expression",
  "author": {
    "name": "Theo Sun",
    "email": "theo.sun.github@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cxn-engine"
  },
  "scripts": {
    "pretest": "bash install_no_trace_deps.sh",
    "prelint": "bash install_no_trace_deps.sh",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "overrides": {
    "@sap/xsenv": "3.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "16.2.4",
    "@types/jest": "27.5.2",
    "@types/node": "16.11.41",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "commitlint": "16.3.0",
    "eslint": "8.18.0",
    "eslint-plugin-jest": "26.5.3",
    "husky": "4.3.8",
    "jest": "27.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.5",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "Apache-2.0"
}
